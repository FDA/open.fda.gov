  left: 22%

.datamap-infographic-empty-data
  text-align: center
  padding-left: 325px
  font-size: 20px
  font-weight: bold

.heatmap-infographic-header-title
  font-size: 21px
  position: relative
  left: 7%

.heatmap-header
  width: 600px
  margin: 50px
  padding-left: 200px

.interactive-infographic-center
  text-align: center

.interactive-infographic-left
  float: left

.interactive-infographic-bold
  font-weight: bold
  white-space: nowrap

.heatmap-infographic-zoom-button
  float: right
  border-style: double
  border-width: 2px
  padding: 8px 10px 10px 10px
