.long-text-formatter
  word-break: breakall
  white-space: normal
  overflow: auto
  padding-top: 20px
  padding-bottom: 20px

.datamap-infographic-header
  font-size: 24px
  font-style: normal

.datamap-infographic-header-params
  white-space: nowrap
  font-size: 22px

.datamap-hr 
  display: block
  margin-top: 0.5em
  margin-bottom: 0.5em
  margin-left: auto
  margin-right: auto
  border-style: inset
  border-width: 1px

.datamap-infographic-header-text-bold
  font-weight: bold
  font-style: normal
  white-space: nowrap

.datamap-infographic
