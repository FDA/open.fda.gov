{
  "selectedState": "VA",
  "countBy": "state.exact",
  "title": "Food enforcement recalls change Year Over Year (YOY)",
  "type": "DataMap",
  "dateField": "report_date",
  "startYear": 2012,
  "dataMapConfig": {
    "geographyConfig": {
      "highlightBorderColor": "#bada55",
      "popupTemplate": "<div class='hoverinfo'>${this.geography.properties.name}\nYOY change %: ${this.this.state.currentValue} => ${this.data.numberOfThings}%",
      "highlightBorderWidth": 3
    },
    "defaultFill": "#F5F5F5",
    "height": 400,
    "width": 800,
    "scope": "usa"
  },
  "gridConfig": {
    "rowScrollTimeout": 0,
    "headerRowHeight": 30,
    "rowHeight": 75,
    "minHeight": 400, 
    "minWidth": 1010,
    "dataGridProperties": [
      { 
        "key": "state",
        "name": "State",
        "width":50,
        "sortable": true 
      },
      { 
        "key": "recalling_firm",
        "name": "Recalling Firm",
        "width":150,
        "sortable": true,
        "formatter": "LongTextFormatter"
      },
      { 
        "key": "reason_for_recall",
        "name": "Reason",
        "width":350,
        "locked": true,
        "formatter": "LongTextFormatter"
      },
      { 
        "key": "product_description",
        "name": "Product Description",
        "width":350,
        "formatter": "LongTextFormatter"
      },
      { 
        "key": "status",
        "name": "Status",
        "width":100,
        "sortable": true
      }
    ]
  }
}