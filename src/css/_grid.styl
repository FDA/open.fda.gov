.col
  // all this extra stuff for ie
  display: inline-block
  -ms-flex: 1 0 0
  flex: auto
  float: left
  min-width: 0

.header-width
  max-width: $gridHeader

.reading-width
  max-width: $gridReading

.grow-none
  flex-grow: 0

.self-auto
  align-self: auto

.self-start
  align-self: flex-start

.self-end
  align-self: flex-end

.self-center
  align-self: center

.self-baseline
  align-self: baseline

.self-stretch
  align-self: stretch

.col:last-child
  margin-right: 0

.row,
.flex-row
  width: 100%

// gotta support old IE
.clearfix
  *zoom: 1

.clearfix:before,
.clearfix:after,
.row:before,
.flex-row:before,
.row:after,
.flex-row:after
  content: ''
  display: table

.clearfix:after,
.row:after,
.flex-row:after
  clear: both

.container
  display: block
  margin-left: auto
  margin-right: auto
  // fallback for ie
  width: 85%

.flex-box,
.flex-row
  display: flex
  flex-direction: column
  flex-wrap: wrap

.flex-row.dir-column,
.flex-box.dir-column
  flex-direction: column

.dir-row
  flex-direction: row

.dir-column-reverse
  flex-direction: column-reverse

.dir-row-reverse
  flex-direction: row-reverse

.align-start
  align-items: flex-start

.align-end
  align-items: flex-end

.align-baseline
  align-items: baseline

.align-center
  align-items: center

.align-stretch
  align-items: stretch

.align-content-start
