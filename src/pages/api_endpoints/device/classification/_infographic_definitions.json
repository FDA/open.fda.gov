{
  "title": "<p class='heatmap-infographic-header'><i class='datamap-infographic-header-text-bold'> Device Classification</i>: by Submission Type</p>",  
  "defaults": {
  },
  "xField": "submission_type_id",
  "yField": "medical_specialty_description",
  "yName": "Drug Category",
  "xName": "Administration Route",
  "type": "Grid",
  "xTermLimit":15,
  "yLabel": "Count of Devices by Submission Type",
  "xLabel": "Count of Devices by Medical Specialty",
  "defs": {
    "1":"510(K)",
    "2": "PMA",
    "3": "Contact ODE",
    "4": "510(K) Exempt"
  },
  "exclusions": ["5","6","7","8"],
  "yLabelStyle": {
    "transform": "rotate(270deg) translateX(-30%) translateY(50%)"
  },
  "gridConfig":{
    "transform": "translate(190,10)",
    "keyFormat": "Manufacturer Type: ${this.yKey}\n Medical Specialty: ${this.xKey}\n",
    "valueFormatter": "${this.value}",
    "colorCodes": {
      "greens": ["#c2e2a5", "#9cc677", "#7bba4d", "#4aaa01", "#177d00"],
      "reds":["#ffa1a4","#ff7c80","#fe5057", "#ED182B", "#bd0000"],
      "zero": "#C7C7C7"
    },
    "theme": {
      "fontSize": 11,
      "fontFamily": "Merriweather,Georgia,serif",
      "fontColor": "#000000"
    },
    "colorScales": {
      "missingValue": "-",
      "zero": "#C7C7C7",
      "isYOY": false,
      "all": {
        "colors": ["#E1F4FF","#04A1FC"],
        "range": [1,700],
        "extremity": "#0189D8"
      }
    },
    "height": 300,
    "width": 985,
    "indexBy": "xTerm",
    "margin": {
        "top": 25,
        "right": 25,
        "bottom": 80,
        "left": 350
    },
    "minValue": -100,
    "maxValue": 100,
    "forceSquare": false,
    "sizeVariation": 0,
    "padding": 0,
    "colors": "nivo",
    "axisBottom": {
        "orient": "bottom",
        "tickSize": 5,
        "tickPadding": 5,
        "tickRotation": -35,
        "legendPosition": "center",
        "legendOffset": 36
    },
    "axisLeft": {
        "orient": "right",
        "tickSize": 5,
        "tickPadding": 5,
        "tickRotation": 0,
        "legendPosition": "center",
        "legendOffset": -40
    },
    "enableGridX": false,
    "enableGridY": true,
    "cellShape": "rect",
    "cellOpacity": 1,
    "cellBorderWidth": 1,
    "cellBorderColor": "inherit:darker(2)",
    "enableLabels": true,
    "labelTextColor": "inherit:darker(5)",
    "animate": true,
    "motionStiffness": 120,
    "motionDamping": 9,
    "isInteractive": true,
    "hoverTarget": "rowColumn",
    "cellHoverOpacity": 1,
    "cellHoverOthersOpacity": 0.5
  }
}