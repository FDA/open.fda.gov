{
  "selectedState": "VA",
  "countBy": "state.exact",
  "title": "<p class='datamap-infographic-header'><i class='datamap-infographic-header-text-bold'>Food Enforcement Recalls</i>: Year Over Year (YOY) % Change by State</p>",
  "subtitle": "Description of Recalls",
  "type": "DataMap",
  "ipURL": "https:\/\/freegeoip.net/json/?callback=",
  "dateField": "report_date",
  "startYear": 2012,
  "colorCodes": {
    "greens": ["#c2e2a5", "#9cc677", "#7bba4d", "#4aaa01", "#177d00"],
    "reds":["#ffa1a4","#ff7c80","#fe5057", "#ED182B", "#bd0000"],
    "zero": "#C7C7C7"
  },
  "legend": {
      "title": "YOY % Change",
      "styles": {
        "float": "right" ,
        "otherBottom": 105,
        "otherTop":18,
        "paddingTop": 67,
        "marginLeft": -7,
        "borderSpacing": ".5em"
      },
      "ranges": [
        {
          "color": "#ED182B",
          "name": "> 1001%"
        },
        {
          "color":  "#fe5057",
          "name":  "501% to 1000%"
        },
        {
          "color": "#ffa1a4",
          "name": "1% to 500%"
        },
        {
          "color": "#C7C7C7",
          "name": "0% (No Change)"
        },
        {
          "color": "#c2e2a5",
          "name": "-1% to -500%"
        },
        {
          "color":  "#7bba4d",
          "name":  "-501% to -1000%"
        },
        {
          "color": "#4aaa01",
          "name": "< -1001%"
        }
      ]
  },
  "slider": {
    "style": {
      "fontSize": "15px",
      "float": "left",
      "height": "340px"
    },
    "handleStyle": {
      "borderColor": "black"
    },
    "trackStyle": {
      "backgroundColor": "#8C8C8C",
      "width": 5
    },
    "dotStyle": {
      "backgroundColor": "white",
      "borderColor": "black"
    }
  },
  "dataMapConfig": {
    "geographyConfigInfo": {
      "highlightBorderColor": "#FFFFFF",
      "highlightFillColor": "rgb(44,205,255)",
      "popupTemplate": "<div class='hoverinfo'>${this.geography.properties.name}\nYOY change %: ${this.this.state.currentValue} => ${this.data.numberOfThings}%",
      "highlightBorderWidth": 3
    },
    "fills": {
      "defaultFill": "#FFFFFF"
    },
    "height": 400,
    "width": 800,
    "scope": "usa",
    "bubblesConfig": {
      "borderWidth": 3,
      "borderOpacity": 1,
      "borderColor": "#080808",
      "popupOnHover": false,
      "fillOpacity": 1,
      "animate": false,
      "highlightOnHover": true,
      "highlightFillColor": "#FFFFFF",
      "exitDelay": 100
    },
    "updateChoroplethOptions":{
      "reset": true
    }
  },
  "gridConfig": {
    "rowScrollTimeout": 0,
    "headerRowHeight": 30,
    "rowHeight": 75,
    "minHeight": 400, 
    "minWidth": 1010,
    "pagination": false,
    "reactTableColumns": [
      {
        "Header": "State",
        "accessor": "state",
        "width":50,
        "className": "long-text-formatter"
      },
      {
        "Header": "Recalling Firm",
        "accessor": "recalling_firm",
        "width":150,
        "className": "long-text-formatter"
      },
      {
        "Header": "Reason",
        "accessor": "reason_for_recall",
        "width":350,
        "className": "long-text-formatter"
      },
      {
        "Header": "Product Description",
        "accessor": "product_description",
        "width":340,
        "className": "long-text-formatter"
      },
      {
        "Header": "Status",
        "accessor": "status",
        "width":110,
        "className": "long-text-formatter"
      }
    ],
    "defaultData": [{"reason_for_recall": "No Data Found"},{},{},{},{},{}]
